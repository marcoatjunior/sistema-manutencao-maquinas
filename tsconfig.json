{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@shared/*": [
        "./src/app/shared/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
      "@environments/*": [
        "./src/environments/*"
      ],
      "@managers/*": [
        "./src/app/managers/*"
      ],
      "@users/*": [
        "./src/app/users/*"
      ],
      "@pieces/*": [
        "./src/app/pieces/*"
      ],
      "@machines/*": [
        "./src/app/machines/*"
      ]
    }
  }
}